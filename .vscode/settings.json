{
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.fixAll.stylelint": "explicit"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.requireConfig": true,
	"files.associations": {
		"*.scss": "tailwindcss"
	},
	"search.exclude": {
		"node_modules": true,
		"dist": true,
		"admin-uat-*": true,
		"app-service-*": true,
		"app-entry-*": true
	},
	"cSpell.words": ["ahooks", "Cascader", "headimg", "misdata", "Popconfirm", "realname"],
	"oxc.enable": true
}
